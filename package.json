{
  "name": "firstorder",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "env $(print-env) nodemon src/index.js --exec babel-node",
    "deploy": "env $(print-env) sls deploy",
    "deploy:prod": "env $(print-env) sls deploy --stage prod",
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "test": "npm install && npm run unit:test && npm run lint",
    "unit:test": "echo \"no test runner specified\"",
    "lint": "./node_modules/.bin/eslint -c .eslintrc.js --ext .js ./src/.",
    "lint:fix": "./node_modules/.bin/eslint -c .eslintrc.js --ext .js ./src/. --fix",
    "lint:nowarn": "./node_modules/.bin/eslint -c .eslintrc.js --ext .js ./src/. --quiet",
    "lint:watch": "./node_modules/.bin/esw -c .eslintrc.js --ext .js ./src/. --watch",
    "lint:watch:fix": "./node_modules/.bin/esw -c .eslintrc.js --ext .js ./src/. --fix --watch",
    "lint:watch:nowarn": "./node_modules/.bin/esw -c .eslintrc.js --ext .js ./src/. --quiet --watch",
    "lint:watch:nowarn:fix": "./node_modules/.bin/esw -c .eslintrc.js --ext .js ./src/. --quiet --watch --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test && npm run lint"
    }
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "@mapbox/mapbox-sdk": "^0.3.0",
    "aws-sdk": "^2.293.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "csv-writer": "^1.2.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "npm": "^6.0.1",
    "pg": "^7.4.1",
    "serve-favicon": "^2.5.0",
    "serverless-http": "^1.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^3.1.4",
    "husky": "^1.0.0-rc.9",
    "nodemon": "^1.12.1",
    "serverless-domain-manager": "^2.4.0",
    "serverless-plugin-git-variables": "^2.0.0",
    "serverless-webpack": "^4.4.0",
    "webpack": "^3.12.0"
  }
}
