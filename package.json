{
  "name": "firstorder",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "env $(print-env) nodemon src/index.js",
    "tag": "./scripts/tag",
    "deploy": "env $(print-env) sls deploy",
    "serve": "node src/index.js",
    "test": "npm run unit:test && npm run lint",
    "unit:test": "echo \"no test runner specified\"",
    "lint": "eslint -c .eslintrc.js --ext .js ./src/.",
    "lint:fix": "eslint -c .eslintrc.js --ext .js ./src/. --fix",
    "lint:nowarn": "eslint -c .eslintrc.js --ext .js ./src/. --quiet",
    "lint:watch": "esw -c .eslintrc.js --ext .js ./src/. --watch",
    "lint:watch:fix": "esw -c .eslintrc.js --ext .js ./src/. --fix --watch",
    "lint:watch:nowarn": "esw -c .eslintrc.js --ext .js ./src/. --quiet --watch",
    "lint:watch:nowarn:fix": "esw -c .eslintrc.js --ext .js ./src/. --quiet --watch --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.3.0",
    "@turf/bbox": "^6.0.1",
    "@turf/centroid": "^6.0.2",
    "@turf/distance": "^6.0.1",
    "@turf/envelope": "^5.1.5",
    "@turf/helpers": "^6.1.4",
    "@turf/invariant": "^6.1.2",
    "aws-sdk": "^2.293.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "csv-writer": "^1.2.0",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.10",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "serverless-http": "^1.5.5"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-watch": "^3.1.4",
    "husky": "^1.0.0-rc.9",
    "nodemon": "^1.12.1",
    "serverless": "^1.32.0",
    "serverless-domain-manager": "^2.4.0",
    "serverless-plugin-git-variables": "^2.0.0"
  }
}
